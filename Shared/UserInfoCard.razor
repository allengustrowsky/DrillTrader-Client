<MudContainer MaxWidth="MaxWidth.Large" style="margin-top: 70px;">
    <MudCard xs="10" sm="6">
        <MudCardHeader>
            <CardHeaderContent>
                @*<MudText Typo="Typo.h6" xs="10" sm="6">Welcome, @newUserInfo.first_name @newUserInfo.last_name</MudText>*@
                <MudText Typo="Typo.h6" xs="10" sm="6">Welcome, @UserState.first_name @UserState.last_name</MudText>
            </CardHeaderContent>
        </MudCardHeader>
        <MudCardContent class="d-flex" style="flex-direction: column; justify-content: center; gap: 25px;">
            @*<MudText Typo="Typo.body1" @bind-Value="newUserInfo.apiKey" style="overflow-wrap: break-word"><b>Api Key</b>: @newUserInfo.apiKey</MudText>
            <MudText Typo="Typo.body1" @bind-Value="newUserInfo.email_address"><b>Email</b>: @newUserInfo.email_address</MudText>*@
            <MudText Typo="Typo.body1" @bind-Value="UserState.apiKey" style="overflow-wrap: break-word"><b>Api Key</b>: @UserState.apiKey</MudText>
            <MudText Typo="Typo.body1" @bind-Value="UserState.email_address"><b>Email</b>: @UserState.email_address</MudText>
        </MudCardContent>
    </MudCard>
</MudContainer>

@code {
    public void LogOut()
    {
        UserState.is_logged_in = false;
        StateHasChanged();
    }
}

